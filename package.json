{
  "name": "@envronment/root-docs",
  "version": "0.0.0",
  "description": "Documentation for all projects of EnVRonment",
  "scripts": {
    "docs:start": "cd docs && pnpm start",
    "docs:build": "cd docs && pnpm build",
    "docs:clear": "cd docs && pnpm clear",
    "docs:serve": "cd docs && pnpm serve",
    "docs:deploy": "cd docs && pnpm run deploy",
    "docs:prod": "pnpm docs:build && pnpm docs:serve",
    "prepare": "husky",
    "pre-commit": "cd docs && pnpm lint:staged",
    "commit-msg": "cd docs && commitlint --edit",
    "prepare-commit-msg": "devmoji --edit --body"
  },
  "keywords": [],
  "author": "Alwyn Mattapullut",
  "license": "Copyright",
  "packageManager": "pnpm@9.1.2",
  "devDependencies": {
    "husky": "9.1.4",
    "@alwyn974/devmoji": "2.5.4",
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/cz-commitlint": "19.4.0",
    "commitizen": "4.3.0"
  }
}
